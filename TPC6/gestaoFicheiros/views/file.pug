extends layout

block content
    .w3-row
        .w3-card-4.w3-margin
            header(class="w3-container w3-teal")
                h3= f.name

            .w3-container
                p Data: #{f.date} 
                p Descrição: #{f.description} 

                img(src="http://localhost:3078/images/" + f.name)

                a.w3-btn.w3-round-large.w3-left.w3-margin.w3-teal.w3-right-align(href='/') Voltar
